    "@types/node": "^24.3.1",
    "carbon-preprocess-svelte": "~0.11.2",
    "eslint-config-love": "^97.0.0",
    "eslint-plugin-svelte": "^3.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
